class bubblesort
{
	static int[] bubblesort(int []inputArray)
	{
		int n = inputArray.length;
		int temp = 0;
		for (int i = 0; i < n; i++)
		{
			for (int j = 1;j < (n-1); j++)
			{
				if (inputArray[j-1] > inputArray[j])
				{
					temp = inputArray[j-1];
					inputArray[j-1] = inputArray[j];
					inputArray[j] = temp;
				}
			}
		}
		return inputArray;
	}
}